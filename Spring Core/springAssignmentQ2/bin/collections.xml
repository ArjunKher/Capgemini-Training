<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="questionList" class="com.springcore.collections.QuestionList" >
		<property name="questionId" value="1" />
		<property name="question" value="What is the full form of jsp?" />
		<property name ="answers">
			<list>
				<value>Java servlet</value>
				<value>Java server page</value>
				<value>Java spring core</value>
			</list>
		</property>
	</bean>
	
	<bean id="questionMap" class="com.springcore.collections.QuestionMap" >
		<property name="questionId" value="1" />
		<property name="question" value="What is the full form of jsp?" />
		<property name ="answers">
			<map>
				<entry key="1" value="Java Servlet"></entry>
				<entry key="2" value="Java Server Page"></entry>
				<entry key="3" value="Java Spring core"></entry>
			</map>
		</property>
	</bean>
	
</beans>